{
  "name": "package-template",
  "private": true,
  "version": "0.0.0",
  "author": "talkohavy@gmail.com",
  "license": "ISC",
  "main": "dist/main.js",
  "types": "dist/main.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./index.d.ts",
        "default": "./index.esm.mjs"
      },
      "require": {
        "types": "./index.d.ts",
        "default": "./index.cjs.cjs"
      }
    }
  },
  "scripts": {
    "clean": "rm -rf node_modules",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "tsc": "tsc -p tsconfig.json",
    "build": "node ./build.config.mjs",
    "test": "node --test"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "restricted"
  },
  "devDependencies": {
    "@eslint/js": "^9.34.0",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.4",
    "@types/node": "^24.3.0",
    "esbuild": "^0.25.9",
    "eslint": "^9.34.0",
    "eslint-plugin-perfectionist": "^4.15.0",
    "eslint-plugin-react-compiler": "19.1.0-rc.2",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "prettier": "^3.6.2",
    "rollup": "^4.49.0",
    "tslib": "^2.8.1",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.41.0"
  }
}
